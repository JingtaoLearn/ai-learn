{
  "auth": {
    "profiles": {
      "openai-codex:default": {
        "provider": "openai-codex",
        "mode": "oauth"
      }
    }
  },
  "models": {
    "mode": "merge",
    "providers": {
      "maestro-anthropic": {
        "baseUrl": "https://maestro.us.jingtao.fun/api/anthropic",
        "apiKey": "${OPENCLAW_API_KEY}",
        "api": "anthropic-messages",
        "models": [
          {
            "id": "github-copilot/claude-opus-4.6-fast",
            "name": "Claude Opus 4.6 (fast mode)",
            "reasoning": true,
            "input": [
              "text"
            ],
            "contextWindow": 200000,
            "maxTokens": 16384
          },
          {
            "id": "github-copilot/claude-sonnet-4.5",
            "name": "Claude Sonnet 4.5",
            "reasoning": true,
            "input": [
              "text"
            ],
            "contextWindow": 200000,
            "maxTokens": 16384
          },
          {
            "id": "github-copilot/claude-haiku-4.5",
            "name": "Claude Haiku 4.5",
            "reasoning": false,
            "input": [
              "text"
            ],
            "contextWindow": 200000,
            "maxTokens": 8192
          },
          {
            "id": "github-copilot/claude-opus-4.6",
            "name": "Claude Opus 4.6",
            "reasoning": true,
            "input": [
              "text"
            ],
            "contextWindow": 200000,
            "maxTokens": 16384
          }
        ]
      },
      "openai-codex": {
        "baseUrl": "https://api.openai.com/v1",
        "api": "openai-completions",
        "models": [
          {
            "id": "gpt-5.3-codex",
            "name": "GPT-5.3 Codex",
            "reasoning": true,
            "input": [
              "text"
            ],
            "contextWindow": 200000,
            "maxTokens": 16384
          }
        ]
      }
    }
  },
  "agents": {
    "defaults": {
      "model": {
        "primary": "maestro-anthropic/github-copilot/claude-opus-4.6-fast",
        "fallbacks": [
          "maestro-anthropic/github-copilot/claude-sonnet-4.5",
          "openai-codex/gpt-5.3-codex"
        ]
      },
      "workspace": "/home/jingtao/.openclaw/workspace",
      "compaction": {
        "mode": "safeguard"
      },
      "maxConcurrent": 4,
      "subagents": {
        "maxConcurrent": 8
      }
    }
  },
  "tools": {
    "allow": [
      "exec",
      "process",
      "read",
      "write",
      "edit",
      "apply_patch",
      "browser",
      "web",
      "web_fetch",
      "web_search",
      "memory",
      "cron"
    ],
    "deny": []
  },
  "messages": {
    "ackReactionScope": "group-mentions"
  },
  "commands": {
    "native": "auto",
    "nativeSkills": "auto"
  },
  "session": {
    "dmScope": "per-channel-peer"
  },
  "gateway": {
    "port": 18789,
    "mode": "local",
    "bind": "loopback",
    "auth": {
      "token": "${OPENCLAW_GATEWAY_TOKEN}"
    },
    "remote": {
      "token": "${OPENCLAW_GATEWAY_TOKEN}"
    }
  },
  "plugins": {
    "slots": {
      "memory": "memory-core"
    },
    "entries": {
      "whatsapp": {
        "enabled": true
      }
    }
  }
}
